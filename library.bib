@article{Lowther2009,
abstract = {OBJECTIVES: To describe our experience using satellite image-based sampling to conduct a health survey of children in an urban area of Lusaka, Zambia, as an approach to sampling when the population is poorly characterized by existing census data or maps. METHODS: Using a publicly available Quickbird image of several townships, we created digital records of structures within the residential urban study area using ArcGIS 9.2. Boundaries were drawn to create geographic subdivisions based on natural and man-made barriers (e.g. roads). Survey teams of biomedical research students and local community health workers followed a standard protocol to enroll children within the selected structure, or to move to the neighbouring structure if the selected structure was ineligible or refused enrollment. Spatial clustering was assessed using the K-difference function. RESULTS: Digital records of 16 105 structures within the study area were created. Of the 750 randomly selected structures, six (1{\%}) were not found by the survey teams. A total of 1247 structures were assessed for eligibility, of which 691 eligible households were enrolled. The majority of enrolled households were the initially selected structures (51{\%}) or the first selected neighbour (42{\%}). Households that refused enrollment tended to cluster more than those which enrolled. CONCLUSIONS: Sampling from a satellite image was feasible in this urban African setting. Satellite images may be useful for public health surveillance in populations with inaccurate census data or maps and allow for spatial analyses such as identification of clustering among refusing households.},
author = {Lowther, Sara a. and Curriero, Frank C. and Shields, Timothy and Ahmed, Saifuddin and Monze, Mwaka and Moss, William J.},
doi = {10.1111/j.1365-3156.2008.02185.x},
file = {:Users/mdg5396/Dropbox/mendeley/Lowther et al. - 2009 - Feasibility of satellite image-based sampling for a health survey among urban townships of Lusaka, Zambia.pdf:pdf},
isbn = {1365-3156 (Electronic)$\backslash$n1360-2276 (Linking)},
issn = {13602276},
journal = {Tropical Medicine and International Health},
keywords = {Geographic information systems,Health survey,Sampling,Satellite imagery,Zambia,satellite},
mendeley-tags = {satellite},
number = {1},
pages = {70--78},
pmid = {19121149},
title = {{Feasibility of satellite image-based sampling for a health survey among urban townships of Lusaka, Zambia}},
volume = {14},
year = {2009}
}
@article{Manski2010b,
abstract = {Rounding is the familiar practice of reporting one value whenever a real number lies in an interval. Uncertainty about the extent of rounding is common when researchers analyze survey responses to numerical questions. The prevalent practice has been to take numerical responses at face value, even though many may in fact be rounded. This paper studies the rounding of responses to survey questions that ask persons to state the percent-chance that some future event will occur. We analyze data from the Health and Retirement Study and find strong evidence of rounding, the extent of rounding differing across respondents. We propose use of a person's response pattern across different questions to infer his rounding practice, the result being interpretation of reported numerical values as interval data. We then bring to bear recent developments on statistical analysis of interval data to characterize the potential consequences of rounding for empirical research. Finally, we propose enrichment of surveys by probing to learn the extent and reasons for rounding.},
author = {Manski, Charles F and Molinari, Francesca},
doi = {10.1198/jbes.2009.08098},
file = {:Users/mdg5396/Dropbox/mendeley/Manski, Molinari - 2010 - Rounding Probabilistic Expectations in Surveys.pdf:pdf},
issn = {0735-0015},
journal = {Journal of Business {\&} Economic Statistics},
keywords = {interval data,partial identification,probabilistic expectations,rounding,survey data},
month = {apr},
number = {2},
pages = {219--231},
pmid = {20368764},
title = {{Rounding Probabilistic Expectations in Surveys.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2847785{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {28},
year = {2010}
}
