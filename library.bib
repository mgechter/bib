@article{Kleven2013,
abstract = {We develop a framework for nonparametrically identifying optimization frictions and structural elasticities using notches—discontinuities in the choice sets of agents—introduced by tax and transfer policies. Notches create excess bunching on the low-tax side and missing mass on the high-tax side of a cutoff, and they are often associated with a region of strictly dominated choice that would have zero mass in a frictionless world. By combining excess bunching (observed response attenuated by frictions) with missing mass in the dominated region (frictions), it is possible to uncover the structural elasticity that would govern behavior in the absence of frictions and arguably capture long-run behavior. We apply our framework to tax notches in Pakistan using rich administrative data. While observed bunching is large and sharp, optimization frictions are also very large as the majority of taxpayers in dominated ranges are unresponsive to tax incentives. The combination of large observed bunching and large frictions implies that the frictionless behavioral response to notches is extremely large, but the underlying structural elasticity driving this response is nevertheless modest. This highlights the inefficiency of notches: by creating extremely strong price distortions, they induce large behavioral responses even when structural elasticities are small. JEL Codes: H31, J22, O12. [ABSTRACT FROM AUTHOR]},
author = {Kleven, Henrik J. and Waseem, Mazhar},
doi = {10.1093/qje/qjt004},
file = {:Users/mdg5396/Dropbox/mendeley/Kleven, Waseem - 2013 - Using notches to uncover optimization frictions and structural elasticities Theory and evidence from Pakistan.pdf:pdf},
isbn = {0033-5533$\backslash$r1531-4650},
issn = {00335533},
journal = {Quarterly Journal of Economics},
number = {2},
pages = {669--723},
title = {{Using notches to uncover optimization frictions and structural elasticities: Theory and evidence from Pakistan}},
volume = {128},
year = {2013}
}
@article{Peters2010,
abstract = {Aggregate total factor productivity depends both on firms� physical$\backslash$nproductivity (technology) and$\backslash$n$\backslash$non the efficiency of the resource allocation. The empirical regularity$\backslash$nthat cross-sectional productivity$\backslash$n$\backslash$ndifferences are more pronounced in underdeveloped economies is often$\backslash$ninterpreted as evidence for the$\backslash$n$\backslash$nimportance of misallocation to explain the cross-country variation$\backslash$nin TFP. This paper argues, that this$\backslash$n$\backslash$nfinding is also informative about differences in technology. I construct$\backslash$na simple endogenous growth$\backslash$n$\backslash$nmodel, where the cross-sectional distribution of productivity and$\backslash$nthe growth rate of aggregate TFP are$\backslash$n$\backslash$njointly determined. As output markets are imperfectly competitive,$\backslash$nthe cross-sectional productivity$\backslash$n$\backslash$ndispersion will reflect the distribution of mark-ups. Mark-ups, however,$\backslash$nnot only cause static misallocation,$\backslash$n$\backslash$nbut they also determine innovation and entry incentives and hence$\backslash$nthe equilibrium growth$\backslash$n$\backslash$nrate. If equilibrium entry is intense (for example because entry barriers$\backslash$nare low), product markets are$\backslash$n$\backslash$ncompetitive, the productivity dispersion across firms is small and$\backslash$nthe aggregate growth rate is high.$\backslash$n$\backslash$nThe cross-country variation in productivity dispersion might therefore$\backslash$nbe a symptom of fundamental$\backslash$n$\backslash$ndifferences in the innovation environment. Using firm-level data from$\backslash$nIndonesia, I present both reduced$\backslash$n$\backslash$nform evidence for this mechanism and estimate the models� structural$\backslash$nparameters. A policy,$\backslash$n$\backslash$nwhich reduces existing entry barriers by 10{\%}, will increase welfare$\backslash$nby 5{\%}. While 10{\%} of these gains are$\backslash$n$\backslash$nattributed to a reduction of static misallocation, 60{\%} stem from a$\backslash$nchange in the equilibrium growth$\backslash$n$\backslash$nrate.},
author = {Peters, Michael},
file = {:Users/mdg5396/Dropbox/mendeley/Peters - 2013 - Heterogeneous Mark-Ups and Endogenous Misallocation.pdf:pdf},
journal = {Working Paper},
keywords = {Michael Peters},
title = {{Heterogeneous Mark-Ups and Endogenous Misallocation}},
year = {2013}
}
