isbn = {9780444529442},
issn = {15734471},
keywords = {Distribution of income,Informal labor market,Labor regulations,Minimum wage,Productivity},
number = {C},
pages = {4657--4702},
publisher = {Elsevier BV},
title = {{Labor regulations, unions, and social protection in developing countries: Market distortions or efficient institutions?}},
