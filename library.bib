journal = {Operations Research},
keywords = {data-driven decision making,heterogeneous treatment effects,minimax regret,mixed integer program,observational study,policy learning},
title = {{Offline Multi-Action Policy Learning: Generalization and Optimization}},
url = {http://arxiv.org/abs/1810.04778}
}
