isbn = {9788578110796},
issn = {1098-6596},
journal = {Journal of Urban Economics},
keywords = {Agglomeration economies,Developing countries,Human capital externalities,Spatial equilibrium,Urbanization},
pages = {1--61},
pmid = {25246403},
title = {{What is different about urbanization in rich and poor countries? Cities in Brazil, China, India, and the United States}},
