journal = {The Cochrane database of systematic reviews},
number = {7},
pages = {CD000371},
pmid = {26202783},
title = {{Deworming drugs for soil-transmitted intestinal worms in children: effects on nutritional indicators, haemoglobin, and school performance.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26202783%5Cnhttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4523932},
volume = {7},
