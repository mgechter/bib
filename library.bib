journal = {Remote Sensing},
keywords = {Built-up land cover,Google Earth Engine,Landsat,Pixel-based image classification,Remote sensing,Urbanization},
number = {8},
title = {{Detecting the boundaries of urban areas in India: A dataset for pixel-based image classification in google earth engine}},
volume = {8},
year = {2016}
}
