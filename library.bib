journal = {Journal of International Economics},
keywords = {Colombia,Industry wages,Political economy,Trade liberalization},
number = {1},
pages = {75--105},
title = {{Trade, wages, and the political economy of trade protection: Evidence from the Colombian trade reforms}},
volume = {66},
year = {2005}
}
