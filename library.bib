title = {{Ordeal Mechanisms in Targeting: Theory and Evidence from a Field Experiment in Indonesia}},
url = {http://www.nber.org/papers/w19127.pdf$\backslash$npapers3://publication/uuid/81E7F630-D8CB-41C3-8AAA-B69DEB28B73A},
year = {2013}
}
