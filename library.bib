@article{Ledvina2010,
abstract = {We compare the number of active firms, i.e. the number of firms producing a positive quantity, in equilibrium across four different models of oligopoly: Cournot and Bertrand with homogeneous or differentiated goods. We concentrate on the linear demand structure with con-stant marginal but asymmetric costs. (With symmetric costs, the results trivialize to all firms active or all firms inactive.) When firms have different costs, we show that, for fixed good type, Cournot always results in more active firms than Bertrand. Moreover, with a fixed market type, differentiated goods result in more active firms than homogeneous goods. Hence, consumers pay for more choice in a Cournot market relative to Bertrand.},
