keywords = {Discrete-choice models,Migration costs,Particulate matter,Residential sorting,Valuation of air quality,Wage-hedonic models},
number = {1},
pages = {1--14},
publisher = {Elsevier},
title = {{Migration and hedonic valuation: The case of air quality}},
url = {http://dx.doi.org/10.1016/j.jeem.2008.08.004},
volume = {58},
year = {2009}
}
