journal = {World Development},
keywords = {India,South Asia,hedonic model,property rights,slums,spatial econometrics},
pages = {184--198},
publisher = {Elsevier Ltd},
title = {{Tenure Security Premium in Informal Housing Markets: A Spatial Hedonic Analysis}},
url = {http://dx.doi.org/10.1016/j.worlddev.2016.08.009},
volume = {89},
year = {2017}
}
