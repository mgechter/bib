issn = {1467789X},
journal = {Obesity Reviews},
keywords = {abdominal obesity,physical activity,visceral adiposity,weight loss},
number = {8},
pages = {1--17},
pmid = {35383401},
title = {{Effect of aerobic exercise on waist circumference in adults with overweight or obesity: A systematic review and meta-analysis}},
volume = {23},
