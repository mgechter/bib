journal = {Journal of the American Statistical Association},
keywords = {Multiple correlation,Nominal-categorical variables,Nonparametric measures of dependence,Ordinal-categorical variables,Partial correlation,Pearson's phi-square},
number = {405},
pages = {157--164},
title = {{Relative entropy measures of multivariate dependence}},
volume = {84},
year = {1989}
}
