isbn = {1091-6490 (Electronic)\r0027-8424 (Linking)},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Adult,Body Height,Child Development,Cross-Sectional Studies,Developing Countries,Educational Status,Female,Humans,India,Indonesia,Infant,Infant, Newborn,Learning,Male,Mothers,Mothers: psychology,Peru,Senegal,Social Class,Young Adult},
number = {Supplement_2},
pages = {17273--80},
pmid = {23045688},
title = {{Socioeconomic gradients in child development in very young children: evidence from India, Indonesia, Peru, and Senegal.}},
