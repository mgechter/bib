@article{Manski2011,
abstract = {Economists studying choice with partial knowledge typically assume that the decision maker places a subjective distribution on unknown quantities and maximizes expected utility. Someone lacking a subjective distribution faces a problem of choice under ambiguity. This article reviews recent research on policy choice under ambiguity, when the task is to choose treatments for a popu- lation. Ambiguity arises when a planner has partial knowledge of treatment response and does not feel able to place a subjective distribution on the unknowns. I first discuss dominance and alter- native criteria for choice among undominated policies. I then illustrate with the choice of a vaccination policy by a planner who has partial knowledge of the effect of vaccination on illness. I next study a class of problems in which a planner may want to cope with ambiguity by diversification, assigning observationally identi- cal persons to different treatments. Lastly, I consider a setting in which a planner should not diversify treatment.},
author = {Manski, Charles F.},
file = {:Users/mdg5396/Dropbox/mendeley/Manski - 2011 - Choosing Treatment Policies Under Ambiguity.pdf:pdf},
issn = {1941-1383},
journal = {Annual Review of Economics},
number = {1},
pages = {25--49},
publisher = {Annual Reviews 4139 El Camino Way, PO Box 10139, Palo Alto, California 94303-0139, USA},
title = {{Choosing Treatment Policies Under Ambiguity}},
volume = {3},
year = {2011}
}
