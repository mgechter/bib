journal = {ISPRS Journal of Photogrammetry and Remote Sensing},
keywords = {Aerial images,Building heights,Frequency analysis,Gabor filters,Level set,Region based active contours,Satellite images,Segmentation,Shadow detection,Shadow extraction},
pages = {437--450},
publisher = {International Society for Photogrammetry and Remote Sensing, Inc. (ISPRS)},
title = {{Satellite images analysis for shadow detection and building height estimation}},
url = {http://dx.doi.org/10.1016/j.isprsjprs.2016.07.006},
volume = {119},
year = {2016}
}
