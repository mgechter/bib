@unpublished{Meager2016,
abstract = {Distributional treatment effects can be crucial for understanding the welfare impact of interventions in development economics, yet they are typically ignored in meta-analysis due to a lack of suitable aggregation methodologies. I develop and implement a new suite of Bayesian hierarchical models and associated pooling metrics to estimate the distributional treatment effects of expanding access to microcredit on household outcomes, and to assess the external validity of the results. This requires imposing a series of interdependent con-straints to ensure that the estimates imply proper outcome distributions in both treatment and control groups. The results show that each study exhibits large treatment effects on the variance of household outcomes, but these effects are not generalizable due to substantial heterogeneity across sites -this contrasts to the results on the average treatment effects, which are negligible but highly generalizable. Aggregating the quantile treatment effects resolves this apparent puzzle: microcredit generally does not affect the distribution of house-hold outcomes below the 75th quantile, and above this point the thickness of the tails and the potential for cross-site heterogeneity prevents reliable extrapolation to the general case. Policymakers should not base decisions about the funding of microcredit interventions on any purported upper tail effects, as we effectively cannot predict the effect on the upper quantiles in the next site. However, pooling metrics applied to the set of quantile effects detect substantial similarities across sites overall, due to strong pooling on the majority of the distribution which is generally unaffected by microcredit. Yaroslav Mukhin, and the audiences of the MIT Economic Development Lunch Seminar, MIT Econometrics Lunch Seminar and the MIT Summer Applied Micro Lunch Series for their suggestions, critiques, and advice. I also thank the authors of the 7 studies that I use in my analysis, and the journals in which they were published, for making their data and code public. All remaining mistakes are my own. This is a working paper, so please send critiques and corrections to rmeager@mit.edu},
author = {Meager, Rachael},
file = {::},
pages = {1--73},
title = {{Aggregating Distributional Treatment Effects : A Bayesian Hierarchical Analysis of the Microcredit Literature}},
year = {2016}
}
