pages = {289--319},
title = {{Spillover Effects in Experimental Data}},
year = {2021}
}
@article{Pritchett2015,
abstract = {Suppose a policymaker in context j is contem-plating expanding some social program in hopes of raising welfare as measured by Y . She con-fronts two types of evidence. First, researchers have used observational data from a represen-tative sample of eligible individuals, i , some of whom self-selected into the program, to estimate the effect of program participation, T , using the following regression: (1) Y ij = $\mu$ j + $\beta$ j T ij + $\eta$ ij . The OLS estimates of $\beta$ j clearly suffer poten-tial bias. Second, the policymaker reviews avail-able experimental studies of similar programs in other contexts ($\beta$ k for k ≠ j) that use random assignment to overcome this bias. This is a stylized description of many real-world decisions, in which policymakers must weigh observational data from the right context against experimental evidence from a differ-ent program and context. This paper attempts to measure the trade-off between internal and external validity empirically, using examples from development economics. The root mean squared error (RMSE) of the prediction of program impact encompasses threats to both internal and external validity. We calculate the RMSE for observational estimates from the relevant context by comparing it to the experimental estimate from within the same study: (2) RMSE ($\beta$ ̂ j},
author = {Pritchett, Lant and Sandefur, Justin},
doi = {10.1257/aer.p20151016},
