@article{Schurter2016,
abstract = {The goal of this paper is to detect collusion and estimate its effect on the seller's revenue in first-price auctions with independent, private valuations. The challenge is that collusion may be difficult to detect because colluders can use a simple and costless strategy to make their bids appear competitive. If the econometrician observes an exogenous shifter of the level of competition at the auction in addition to the winning bids, a statistical test for collusion can be formulated as a test of independence between the exogenous shifter and the valuations that rationalize the bids under the null hypothesis that it is not colluding. Simulations confirm that this test performs well even when colluders attempt to disguise their behavior. I then adopt a multiple hypothesis testing framework to simultaneously test for collusion bidder-by-bidder. By controlling the probability of making one or more type I errors, the set of rejected hypotheses forms a lower confidence bound on the set of colluders. In order to produce a lower confidence bound on the cost of collusion, I use consistent estimates of the bidders' valuation distributions to numerically solve for the seller's expected revenues in auctions with and without collusion. As an example of this identification strategy, I use exogenous variation in the reserve prices at British Columbia's timber auctions to estimate the extent of collusion in the years preceding a lumber trade dispute between the US and Canada.},
author = {Schurter, Karl},
pages = {1--84},
title = {{Identification and Inference in First-Price Auctions with Collusion}},
year = {2016}
}
