issn = {0012-9682},
journal = {Econometrica},
keywords = {alternative,bias correction,local polynomials,regression discontinuity,robust inference},
number = {6},
pages = {2295--2326},
title = {{Robust nonparametric confidence intervals for regression-discontinuity designs}},
