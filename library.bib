file = {:Users/mdg5396/Dropbox/mendeley/Semenova et al. - 2023 - Estimation and Inference on Heterogeneous Treatment Effects in High-Dimensional Dynamic Panels under Weak Depen.pdf:pdf},
issn = {17597331},
journal = {Quantitative Economics},
pages = {471--510},
title = {{Estimation and Inference on Heterogeneous Treatment Effects in High-Dimensional Dynamic Panels under Weak Dependence}},
url = {http://arxiv.org/abs/1712.09988},
volume = {14},
year = {2023}
}
